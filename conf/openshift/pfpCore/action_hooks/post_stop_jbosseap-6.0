#!/bin/bash

printJvmCount() {
    mkdir  -p ~/${openshift.jboss.cartridge.type}/tmp/log
    echo `date` > ~/${openshift.jboss.cartridge.type}/tmp/log/post_stop_jbosseap.log
    for jProc in `ps -C java -o pid=`;
    do
        echo "java process id = $jProc\t" >> ~/${openshift.jboss.cartridge.type}/tmp/log/post_stop_jbosseap.log
    done
}
printJvmCount

